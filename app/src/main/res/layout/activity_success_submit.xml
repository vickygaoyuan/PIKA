<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.vickygao.masproject.DetailCheckActivity"
    >

    <TextView
        android:layout_marginTop="12dp"
        android:textSize="32dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Successfully Submitted"
        android:id="@+id/textView16"
        android:layout_alignParentTop="true"
        android:paddingBottom="10pt"
        android:layout_centerHorizontal="true" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="48dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Item:"
        android:id="@+id/textView5"
        android:layout_toEndOf="@+id/textView5"
        android:layout_below= "@+id/textView16"/>

    <TextView
        android:textSize="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitItem"
        android:layout_below= "@+id/textView16"
        android:layout_alignTop="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expected Delivery Time:"
        android:id="@+id/textView7"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/submitItem" />

    <TextView
        android:textSize="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitTime"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignStart="@+id/submitItem" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pickup Address:"
        android:id="@+id/textView8"
        android:layout_below="@+id/submitTime"
        android:layout_alignParentStart="true" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitPickupa"
        android:layout_below="@+id/submitTime"
        android:layout_alignStart="@+id/submitTime" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitDelivera"
        android:layout_below="@+id/submitPickupa"
        android:layout_alignStart="@+id/submitPickupa" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deliver Address:"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fee:"
        android:id="@+id/textView9"
        android:layout_below="@+id/submitDelivera"
        android:layout_alignParentStart="true" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitFee"
        android:layout_below="@+id/submitDelivera"
        android:layout_alignStart="@+id/submitDelivera" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Giver:"
        android:id="@+id/textView13"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/submitFee" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitGiver"
        android:layout_below="@+id/submitFee"
        android:layout_alignStart="@+id/submitFee" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:textColor="@color/primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Giver&apos;s Phone Num:"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true" />

    <TextView
        android:textSize="18dp"
        android:layout_marginTop="18dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/submitGivern"
        android:layout_below="@+id/submitGiver"
        android:layout_alignStart="@+id/submitGiver" />

    <Button
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:text="OK"
        android:background="@color/primary"
        android:textColor="@color/white"
        android:id="@+id/okBtn"
        android:textSize="14dp"
        android:layout_marginBottom="100dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="200dp"/>


</RelativeLayout>
